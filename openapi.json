{
  "openapi": "3.0.0",
  "info": {
    "title": "Redash API Client",
    "version": "0.1.0",
    "contact": {
      "name": "Takuya Arita",
      "email": "takuya.arita@gmail.com",
      "url": "https://github.com/ariarijp"
    },
    "license": {
      "name": "MIT"
    }
  },
  "security": [
    {
      "APIKey": []
    }
  ],
  "servers": [],
  "paths": {
    "/api/queries": {
      "get": {
        "parameters": [
          {
            "name": "page",
            "in": "query",
            "required": false,
            "schema": {
              "type": "integer"
            }
          },
          {
            "name": "page_size",
            "in": "query",
            "required": false,
            "schema": {
              "type": "integer"
            }
          },
          {
            "name": "order",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "q",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "A list of queries",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Queries"
                }
              }
            }
          }
        }
      }
    },
    "/api/queries/{id}": {
      "get": {
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "An object of query",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Query"
                }
              }
            }
          }
        }
      }
    },
    "/api/queries/{id}/refresh": {
      "post": {
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "An object of job",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Job"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "links": {},
    "callbacks": {},
    "schemas": {
      "Queries": {
        "properties": {
          "count": {
            "type": "integer"
          },
          "page": {
            "type": "integer"
          },
          "page_size": {
            "type": "integer"
          },
          "results": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Query"
            }
          }
        }
      },
      "Query": {
        "properties": {
          "last_modified_by_id": {
            "type": "integer"
          },
          "latest_query_data_id": {
            "type": "integer"
          },
          "schedule": {
            "type": "integer"
          },
          "is_archived": {
            "type": "boolean"
          },
          "retrieved_at": {
            "type": "string"
          },
          "updated_at": {
            "type": "string"
          },
          "user": {
            "$ref": "#/components/schemas/User"
          },
          "query": {
            "type": "string"
          },
          "is_draft": {
            "type": "boolean"
          },
          "id": {
            "type": "integer"
          },
          "description": {
            "type": "string"
          },
          "runtime": {
            "type": "float"
          },
          "name": {
            "type": "string"
          },
          "created_at": {
            "type": "dateTime"
          },
          "version": {
            "type": "integer"
          },
          "query_hash": {
            "type": "string"
          },
          "api_key": {
            "type": "string"
          },
          "options": {
            "$ref": "#/components/schemas/Options"
          },
          "data_source_id": {
            "type": "integer"
          }
        }
      },
      "User": {
        "properties": {
          "auth_type": {
            "type": "string"
          },
          "created_at": {
            "type": "dateTime"
          },
          "name": {
            "type": "string"
          },
          "gravatar_url": {
            "type": "string"
          },
          "updated_at": {
            "type": "dateTime"
          },
          "id": {
            "type": "integer"
          },
          "groups": {
            "type": "array",
            "items": {
              "type": "integer"
            }
          },
          "email": {
            "type": "string"
          }
        }
      },
      "Options": {
        "properties": {
          "parameters": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Parameter"
            }
          }
        }
      },
      "Parameter": {
        "properties": {
          "global": {
            "type": "string"
          },
          "type": {
            "type": "string"
          },
          "name": {
            "type": "string"
          },
          "value": {
            "type": "string"
          },
          "title": {
            "type": "string"
          }
        }
      },
      "Job": {
        "properties": {
          "job": {
            "type": "object",
            "properties": {
              "status": {
                "type": "integer"
              },
              "error": {
                "type": "string"
              },
              "id": {
                "type": "string"
              },
              "query_result_id": {
                "type": "integer"
              },
              "updated_at": {
                "type": "dateTime"
              }
            }
          }
        }
      }
    },
    "securitySchemes": {
      "APIKey": {
        "type": "apiKey",
        "name": "api_key",
        "in": "query"
      }
    }
  },
  "tags": []
}